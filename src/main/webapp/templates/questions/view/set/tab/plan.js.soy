{namespace questions.view.set.tab.plan}

/**
 * Full tab
 * @param plan
 * @param? deadLineLink
 */
{template .full}
{if $deadLineLink != null and $deadLineLink.length > 0}
	<div>
		<a href="{$deadLineLink}" class="medium button" data-absolute>Create deadline with this set</a>
	</div>
{/if}
{call shared.data.display}
	{param data: $plan /}
{/call}
{/template}
